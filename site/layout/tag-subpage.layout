{{ define "tag-subpage"}}
{{ template "head" .}}

<body>
    {{template "header" .}}

    <div class="body">
        <article>
            <section class="tagged-posts">
                {{range .SpecificTagTemplates}}
                <a href="/{{.CompleteURL}}">{{.Frontmatter.Title}}</a>
                {{end}}
            </section>
        </article>
    </div>

    {{template "footer" .}}
</body>

</html>

{{ end}}